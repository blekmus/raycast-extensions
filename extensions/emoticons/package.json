{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "emoticons",
  "title": "Emoticon Selector",
  "description": "A selection of Emoticons or Kaomojis that can be used to express yourself more using text.",
  "icon": "command-icon.png",
  "author": "blekmus",
  "license": "MIT",
  "categories": [
    "Media"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Show Emoticons",
      "description": "Show all Emoticons",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}